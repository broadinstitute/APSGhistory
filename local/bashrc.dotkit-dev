# Set up dotkit
eval `/broad/tools/dotkit-dev/init`

if shopt -q login_shell; then 
  # Login Shells
  if [[ "x$RUN_ONCE" == "x" ]]; then
    export RUN_ONCE="1"
    use -q default
  fi
fi

if [[ "$-" =~ "i" ]]; then
  # Interactive Shells
  use -q default++
fi

# Source user configuration
if [ -f $HOME/.my.bashrc ]; then
  . $HOME/.my.bashrc
fi

