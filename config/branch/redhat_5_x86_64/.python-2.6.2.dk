#c devel
#d Python 2.6.2
#rootpath /broad/tools/Linux/redhat_5_x86_64/pkgs/python_2.6.2

# Clear out any python versions lingering around
unuse -q `dk_rep '*python*'`

# Packages python needs
dk_op -q .oracle-instaclient-11.1

# Python locations.
dk_alter PATH /broad/tools/Linux/redhat_5_x86_64/pkgs/python_2.6.2/bin
dk_alter LD_LIBRARY_PATH /broad/tools/Linux/redhat_5_x86_64/pkgs/python_2.6.2/lib


