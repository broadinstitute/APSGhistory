unuse -q `dk_rep ".arachne*"`
unuse -q `dk_rep "Arachne*"`

# Dependencies
dk_op -q .boost-1.38.0
dk_op -q .gcc-4.3.3

# Environment
dk_alter PATH /wga/scr2/ArachneReleases/ReleaseArachne-1-16/bin
dk_alter PATH /wga/scr2/ArachneReleases/ReleaseArachne-1-16/solexa
dk_setenv SOLEXA_PIPELINE_DIR /slxa/pipelineOutput/farm
