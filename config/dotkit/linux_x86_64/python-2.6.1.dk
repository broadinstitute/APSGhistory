#c versioned-languages
#d Python 2.6.1

# Clear out any python versions lingering around
unuse -q `dk_rep 'python*'`

# scipy needs this
dk_op -q math

# Python locations.
dk_alter PATH /broad/tools/Linux/x86_64/pkgs/python_2.6.1/bin
dk_alter LD_LIBRARY_PATH /broad/tools/Linux/x86_64/pkgs/python_2.6.1/lib
