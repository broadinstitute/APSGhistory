dk_alter C_INCLUDE_PATH /broad/tools/Linux/x86_64/pkgs/ImageMagick_6.4.9-10/include
dk_alter CPLUS_INCLUDE_PATH /broad/tools/Linux/x86_64/pkgs/ImageMagick_6.4.9-10/include



