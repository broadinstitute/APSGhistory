#!/usr/bin/expect

set host [lindex $argv 0]
set passw [lindex $argv 1]
set enpassw [lindex $argv 2]
set user [lindex $argv 3]
set date [exec date +%Y%m%d]

spawn ssh "$user@$host"
expect -nocase "password:"
send "$passw\r"
expect "$host>"
send "en\r"
expect "Password:"
send "$enpassw\r"
expect "$host#"
send "copy run tftp\r"
expect "remote host"
send "69.173.115.212\r"
expect "filename"
send "$date/$host\r"
expect "$host#"
send "logout\r"
expect EOF
exit 0
