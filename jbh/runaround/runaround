	}

hosts = hostspecs.keys()
hosts.sort()
basecmd = "ssh %%HOST%%.broad.mit.edu"

opts,args = getopt.getopt(sys.argv[1:],"h:H:x:X:e:q:")

# Check command line options
for opt in opts:
    optname,optval = opt

#
# -h host1,host2,host3,... (include hosts)
#
    if optname == "-h":
      	hosts = string.split(optval,",")

#
# -H keyword (include hosts)
#
    elif optname == "-H":
        newhosts = []
	for host in hosts:
	    if hostspecs[host].count(optval):
                newhosts.append(host)
        hosts = newhosts

#
# -x host1,host2,host3,... (exclude hosts)
#
    elif optname == "-x":
        removehosts = string.split(optval,",")
        for rmhost in removehosts:
            try:
                hosts.remove(rmhost)
            except ValueError:
                continue

#
# -X keyword (exclude hosts)
#
    elif optname == "-X":
        removehosts = []
        for host in hosts:
            if hostspecs[host].count(optval):
                removehosts.append(host)
        for rmhost in removehosts:
            try:
                hosts.remove(rmhost)
            except ValueError:
                continue
#
# -e 'command' (excute this instead of "ssh %%HOST%% ")
#
    elif optname == "-e":
        basecmd = optval

#
# -q (quiet, no output except for command output)
#
    elif optname == "-q":
	quiet = 1 

    else:
        # should never be reached, unless option list is out of sync
        print "illegal option", optname

# bug fix - if you use -e you can get a null argument otherwise
if args:
    cmd = basecmd + " '" + string.join(args," ") + "'"
else:
    cmd = basecmd

for host in hosts:
    mycmd = string.replace(cmd,"%%HOST%%",host)
    # work around cron's handling of percent signs
    mycmd = string.replace(mycmd,"@@HOST@@",host)
    if quiet != 1:
	 print "-->",host,"starting run",time.strftime("(%Y%m%d-%H:%M:%S):"),mycmd
    sys.stdout.flush()                  # clear buffer before system call
    os.system(mycmd)
    if quiet != 1:
    	print "<--",host,"ending run"
    print ""
